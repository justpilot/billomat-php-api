{
  "name": "justpilot/billomat-php-api",
  "description": "Modern PHP 8.4+ SDK for the Billomat API, powered by Symfony components.",
  "type": "library",
  "require": {
    "php": ">=8.4",
    "symfony/http-client": "^7.3",
    "symfony/serializer": "^7.3",
    "symfony/options-resolver": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.4",
    "symfony/dotenv": "^7.3"
  },
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "Justpilot\\Billomat\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Justpilot\\Billomat\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Dimitri Dovgan",
      "email": "dimitri@justpilot.io"
    }
  ],
  "minimum-stability": "stable"
}
