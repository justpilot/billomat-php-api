{
  "name": "justpilot/billomat-php-api",
  "description": "Unofficial PHP 8.4+ SDK for the Billomat API, powered by Symfony components.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.4",
    "symfony/http-client": "^7.3 || ^8.0",
    "symfony/serializer": "^7.3 || ^8.0",
    "symfony/options-resolver": "^7.3 || ^8.0",
    "symfony/http-foundation": "^7.3 || ^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.4",
    "symfony/dotenv": "^7.3 || ^8.0",
    "symfony/var-dumper": "^7.3 || ^8.0",
    "fakerphp/faker": "^1.24"
  },
  "autoload": {
    "psr-4": {
      "Justpilot\\Billomat\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Justpilot\\Billomat\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Dimitri Dovgan",
      "email": "dimitri@justpilot.io"
    }
  ]
}